{
  "cli": {
    "version": ">= 5.7.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "EXPO_PUBLIC_API_URL": "https://uat.monnyka.top"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "release": {
      "env": {
        "APP_VARIANT": "release",
        "EXPO_PUBLIC_API_URL": "https://prod.monnyka.top"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
